[run]
branch = True
include =
    ./dotfiles/*
omit =
    ./.idea/*
    ./.pytest_cache/*
    ./.vscode/*
    ./alembic/*
    ./files/*
    ./htmlcov/*
    ./junit/*
    ./scripts/*
    ./tests/*
    ./utility-repo-scripts/*
    */__pycache__/*
    ./setup.py
concurrency = thread,greenlet
[report]
precision = 2
show_missing = True
skip_empty = True
sort = Name
[html]
directory = ./htmlcov
skip_empty = True
title = dotfiles Coverage Report
[xml]
output = ./junit/coverage.xml
